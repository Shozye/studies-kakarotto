[ a ^ b mod c
? 1234567890
? 1234567890987654321
? 987654321
> 674106858
1234567890 1234567890987654321 987654321
]
PROCEDURE power(a,b,c,d) IS
VAR pot,wyk,o
BEGIN
    d:=1;
    wyk:=b;
    pot:=a%c;
    WHILE wyk>0 DO
	o:=wyk%2;
	IF o=1 THEN
	    d:=d*pot;
	    d:=d%c;
	ENDIF
	wyk:=wyk/2;
	pot:=pot*pot;
	pot:=pot%c;
    ENDWHILE
END

PROGRAM IS
VAR a,b,c,d
BEGIN
   READ a;
   READ b;
   READ c;
   power(a,b,c,d);
   WRITE d;
END
